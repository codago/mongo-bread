<% include partials/head %>
  <% include partials/header %>
  <% console.log(selectedData) %>
    <div class=container>
      <div class="form-group">
      <form class="" action="" method="POST">
        <input type="hidden" name="_id" value="<%= selectedData._id %>">
          <br>String<br>
          <input class="form-control" type="text" name="datastring" value="<%= selectedData.datastring %>">
        </div>

        <div class="form-group">
          <br>Integer<br>
          <input class="form-control" type="number" name="datainteger" value="<%= selectedData.datainteger %>">
        </div>

        <div class="form-group">
          <br>Float<br>
          <input class="form-control" type="number" name="datafloat" step="0.01" value="<%= selectedData.datafloat %>">
        </div>

        <div class="form-group">
          <br>Date<br>
          <input class="form-control" type="date" name= "datadate"  value="<%=selectedData.datadate %>">
        </div>

        <div>
          <br>Boolean<br>
            <select class="form-control" name="databoolean">
              <option value="true" <%= selectedData.databoolean === true ? ' selected' : '' %> >True</option>
              <option value="false" <%= selectedData.databoolean  === false ? ' selected' : '' %> >False</option>
          </select>
        </div>
        <br><br>
        <button class="btn btn-primary" type="submit" name="button">Update</button>
      </form>
    </div>
<% include partials/footer %>
