<form class="form-horizontal" method="post">
 <div class="form-group">
   <label class="control-label col-sm-2" for="string">String:</label>
   <div class="col-sm-10">
     <input id="string" type="text" name = "string" class="form-control" placeholder="Input Data (string)"required>
   </div>
 </div>
 <div class="form-group">
   <label class="control-label col-sm-2" for="number">Integer:</label>
   <div class="col-sm-10">
     <input type="number" step="1" name = "integer" step="1" class="form-control" placeholder="Integer number" required>
   </div>
 </div>
 <div class="form-group">
   <label class="control-label col-sm-2" for="number">Float:</label>
   <div class="col-sm-10">
     <input type="number" class="form-control" name="float" step="0.01" placeholder="Enter float" required>
   </div>
 </div>
 <div class="form-group">
   <label class="control-label col-sm-2" for="date">Date:</label>
   <div class="col-sm-10">
     <input type="date" class="form-control" name="date" placeholder="Enter date" required>
   </div>
 </div>
 <div class="form-group">
   <label class="control-label col-sm-2" for="Boolean">Boolean:</label>
   <div class="col-sm-10">
     <select name="boolean" class="form-control">
       <option>Choose the Boolean</option>
       <option>true</option>
       <option>false</option>
     </select>
   </div>
 </div>
 <div class="form-group">
   <div class="col-sm-offset-2 col-sm-10">
     <button type="submit" class="btn btn-default">Submit</button>
   </div>
 </div>
</form>
